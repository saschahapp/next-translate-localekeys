{
  "name": "next-translate-localekeys-generator",
  "version": "0.0.0",
  "description": "generates typesafe locale keys for next-translate package with auto completion and vsc comments support",
  "main": "./dist/controller.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "tsc -p ./tsconfig.json",
    "generate:example:one": "node . next-translate-localekeys-generate --rootDir ./examples/locales/en --outDir ./examples/generated",
    "generate:example:two": "node . next-translate-localekeys-generate --rootDir ./examples/locales/en --outDir ./examples/generated --translationsEnabled",
    "generate:example:three": "node . next-translate-localekeys-generate --rootDir ./examples/locales/en --outDir ./examples/generated --typescriptEnabled",
    "generate:example:four": "node . next-translate-localekeys-generate --rootDir ./examples/locales/en --outDir ./examples/generated --typescriptEnabled --translationsEnabled"
  },
  "keywords": [],
  "author": "Sascha Happ",
  "license": "ISC",
  "dependencies": {
    "commander": "^9.3.0",
    "node": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.36",
    "prettier": "^2.6.2"
  },
  "bin": {
    "next-translate-localekeys-generate": "./dist/controller.js"
  }
}
